{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "LogicalAddress",
  "description": "A logical address to a service",
  "type": "object",
  "properties": {
    "id": {
      "description": "The unique identifier for a service consumer",
      "type": "integer"
    },
    "hsaId": {
      "description": "The HsaId",
      "type": "string"
    },
    "description": {
      "description": "Description for the service consumer",
      "type": "string"
    },
    "required": ["id"]
  }
}
