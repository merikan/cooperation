{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "LogicalAddress",
  "description": "A logical address to a service",
  "type": "object",
  "properties": {
    "id": {
      "description": "The unique identifier for a logical address",
      "type": "integer"
    },
    "logicalAddress": {
      "description": "The address",
      "type": "string"
    },
    "description": {
      "description": "Description for the address",
      "type": "string"
    },
    "required": ["id"]
  }
}
